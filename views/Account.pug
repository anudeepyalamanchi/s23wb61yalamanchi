extends layout

block content
    h1= title
    -let results   = [{"AccountNumber": "1234", "AccountType": "SAVING", "AccountHolder": "Anudeep",}, { "AccountNumber": "2345", "AccountType": "JOINT", "AccountHolder": "Mintu",}, {"AccountNumber": "3456", "AccountType": "CURRENT", "AccountHolder": "yalamanchi",},]
    div(class= "container-fluid")
        div(class = "row")  
            each item, index in results  
                div(class= "col-sm-2", onclick=`account(${index})`) AccountNumber:  #{item.AccountNumber}       
                div(class= "col-sm-2", onclick=`account(${index})`) AccountType:  #{item.AccountType}   
                div(class= "col-sm-2", onclick=`account(${index})`) AccountHolder:   #{item.AccountHolder}
    script.
        function account(itemNum){
        alert(`You purchased item ${itemNum+1}`)
        }